#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i,n,a[100];
	setbuf(stdout,NULL);
	printf("Enter the limit of array: \n");
	scanf("%d",&n);
	printf("Enter the values: \n");
	for(i=0;i<n;i++){
		scanf("%d",&a[i]);
	}


	return EXIT_SUCCESS;
}
