#include <stdio.h>
#include <stdlib.h>

int main(void) {
	float mark;
	printf("Enter the mark:");
	scanf("%f",&mark);
	if(mark<50){
		printf("Failed");
	}
	else if(mark<60 && mark>49){
		printf("E");
	}
	else if(mark<70 && mark>59){
			printf("D");
		}
	else if(mark<80 && mark>69){
			printf("C");
		}
	else if(mark<90 && mark>79){
			printf("B");
		}
	else (mark>90){
			printf("A");
		}
	return EXIT_SUCCESS;
}
