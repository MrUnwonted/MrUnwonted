#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num;
	printf("Enter the marks:");
	scanf("%d",&num);
	if(num>50){
		printf("You have passed");
	}
	else{
		printf("You have failed");
	}
	return EXIT_SUCCESS;
}
