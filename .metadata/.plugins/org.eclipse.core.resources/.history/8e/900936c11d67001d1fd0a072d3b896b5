#include <stdio.h>
#include <stdlib.h>

int main(void) {

	int i,temp,a[20];
	setbuf(stdout,NULL);
	printf("Enter the values:");
	for(i=0;i<5;i++){
		scanf("%d",&a[i]);
	}
	printf("The sorted array is:");
	for(i=0;i<5;i++){
		if(a[0]>a[i]){
			temp=a[0];
			a[0]=a[i];
			a[i]=temp;
		}
	}
	return EXIT_SUCCESS;
}
