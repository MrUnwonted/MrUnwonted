#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,num,result=0;
	printf("Enter a number:");
	scanf("%d",&num);
	for(i=1;i<num;i++){
		result= i*num;
	}
	printf("The table is:\n");
	printf("%d * %d = %d",i,num,result);
	return EXIT_SUCCESS;
}
