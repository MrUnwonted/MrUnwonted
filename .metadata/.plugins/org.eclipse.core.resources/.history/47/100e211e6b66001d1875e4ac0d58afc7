#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,j,sum=0;
	setbuf(stdout,NULL);
	for(i=1;i<=5;i++){
		for(j=0;j<i;j++){
		sum+=i;
		}
	}
	printf("Sum is %d",sum);
	return EXIT_SUCCESS;
}
