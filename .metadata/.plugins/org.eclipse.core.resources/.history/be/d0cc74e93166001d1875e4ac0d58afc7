#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int num1,num2;
	setbuf(stdout, NULL);
	printf("Enter 2 numbers");
	scanf("%d%d", &num1, &num2);
	printf("The numbers are %d%d", num1, num2);
	return EXIT_SUCCESS;
}
