#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,sum;
	stdbash(stdout,NULL);
	for(i=1;i<=5;i++){
		sum+=i;
		printf("Sum is %d",sum);
	}
	return EXIT_SUCCESS;
}
